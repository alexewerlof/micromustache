{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "newLine": "LF",
    "listEmittedFiles": true,
    "target": "es5",
    "module": "commonjs"
  }
}
